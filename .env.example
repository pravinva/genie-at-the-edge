# AWS Configuration for Databricks/S3 Access
# Copy this to .env and fill in your actual values
# NEVER commit .env to git

# AWS Credentials (for S3, DynamoDB, etc.)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here_if_using_temporary_credentials
AWS_REGION=us-east-1

# Databricks Configuration
DATABRICKS_HOST=https://workspace.cloud.databricks.com
DATABRICKS_TOKEN=your_databricks_token_here
DATABRICKS_WAREHOUSE_ID=4b9b953939869799

# Lakebase Configuration
LAKEBASE_USER=your_lakebase_user
LAKEBASE_PASSWORD=your_lakebase_password
LAKEBASE_CATALOG=lakebase
LAKEBASE_SCHEMA=agentic_hmi

# Ignition Configuration
IGNITION_HOST=localhost
IGNITION_HTTP_PORT=8088
IGNITION_HTTPS_PORT=8043
IGNITION_WEBHOOK_BASE=http://localhost:8088/system/webdev/databricks/webhooks

# ML Model Configuration
ML_MODEL_ANOMALY_PATH=models:/equipment_anomaly_detector/production
ML_MODEL_RECOMMENDATION_PATH=models:/equipment_recommendation_engine/production
ML_BATCH_INTERVAL_SECONDS=10
ML_ANOMALY_THRESHOLD=0.7
ML_CONFIDENCE_THRESHOLD=0.6

# Feature Flags
ENABLE_WEBHOOKS=true
ENABLE_EDGE_DETECTION=false
ENABLE_GENIE_ON_DEMAND=true
WEBHOOK_RETRY_ATTEMPTS=3
WEBHOOK_TIMEOUT_SECONDS=5